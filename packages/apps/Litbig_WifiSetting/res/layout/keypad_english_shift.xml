    <Button android:id="@+id/key_39" android:text="N" android:textColor="#00000000" android:background="@drawable/btn_u_n"/>
    <Button android:id="@+id/key_40" android:text="O" android:textColor="#00000000" android:background="@drawable/btn_u_o"/>
    <Button android:id="@+id/key_41" android:text="P" android:textColor="#00000000" android:background="@drawable/btn_u_p"/>
